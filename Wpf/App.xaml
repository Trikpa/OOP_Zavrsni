<Application
	x:Class="Wpf.App"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:local="clr-namespace:Wpf"
	StartupUri="SettingsWindow.xaml">
	<Application.Resources>
		<BitmapImage x:Key="imgDefaultProfilePicture" UriSource="./Resources/default-profile-picture-100px.png" />
		<BitmapImage x:Key="imgSettings" UriSource="./Resources/settings_icon-24px.png" />
		<BitmapImage x:Key="imgStarFilled" UriSource="./Resources/star_filled.png" />
		<BitmapImage x:Key="imgStarUnfilled" UriSource="./Resources/star_unfilled.png" />
		<BitmapImage x:Key="imgCheck" UriSource="./Resources/check.png" />
		<BitmapImage x:Key="imgCross" UriSource="./Resources/cross.png" />
		<BitmapImage x:Key="imgFootballField" UriSource="./Resources/football_field.jpg" />
	</Application.Resources>
</Application>
